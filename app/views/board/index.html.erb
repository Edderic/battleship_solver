<%= form_tag("/search", method: "get") do %>
  <ul id="ships">
    <span>Sunken ships</span>
    <% [5,4,3,3,2].each do |item| %>
      <li>
      <%= label_tag("ship_#{item}".to_sym, "#{item}:") %>
      <%= check_box_tag("ship_#{item}".to_sym) %>
      </li>
    <% end %>
  </ul>
<% end %>
<%= render partial: 'table', locals: {reduced_table: @reduced_table, abs_freqs: @abs_freqs}  %>
